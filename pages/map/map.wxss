/* 地图 */
.map_content {
    position: relative;
}

.map_nav_box {
    width: 702rpx;
    height: 300rpx;
    background: #fff;
    border-radius: 12rpx;
    position: absolute;
    bottom: 100rpx;
    left: 26rpx;
    box-shadow: 0rpx 0rpx 14rpx rgba(34, 25, 25, 0.3);
    -webkit-box-shadow: 0rpx 0rpx 14rpx rgba(34, 25, 25, 0.3);
}

/* 网点导航 */
.map_nav_tit {
    font-size: 30rpx;
    font-weight: 500;
    color: #252525;
    margin-left: 25rpx;
    margin-top: 25rpx;
}

.map_nav_time_icon,
.map_nav_near_icon {
    width: 30rpx;
    height: 30rpx;
    margin-left: 20rpx;
    margin-top: 25rpx;
    float: left;
}

.map_nav_time_num,
.map_nav_near_tit {
    width: 300rpx;
    font-size: 24rpx;
    color: #999999;
    float: left;
    margin-top: 25rpx;
    margin-left: 13rpx;
}


.map_nav_time_m {
    font-size: 24rpx;
    color: #666666;
    float: left;
    margin-top: 25rpx;
}

.icon_x {
    width: 20rpx;
    height: 20rpx;
    float: left;
    margin-top: 30rpx;
    margin-left: 53%;
}

.map_nav_btn {
    width: 100%;
    height: 90rpx;
    margin-top: 29rpx;
}
.top_line{
    width: 100%;
    height: 1rpx;
    background: #f1f1f1;
    float: left;
}
.map_nav_btn_phone {
    width: 350rpx;
    height: 90rpx;
    float: left;
    margin-top: 10rpx;
}

.map_phone_icon {
    width: 60rpx;
    height: 60rpx;
    float: left;
    margin-left: 55rpx;
    margin-top: 18rpx;
}

.map_phone_title {
    font-size: 26rpx;
    font-weight: 500;
    color: #252525;
    float: left;
    margin-top: 30rpx;
    margin-left: 35rpx;
}

.bottom_line {
    width: 1rpx;
    height: 45rpx;
    float: left;
    background: #f1f1f1;
    margin-top: 32rpx;
}

.map_nav_btn_add {
    width: 345rpx;
    height: 90rpx;
    float: left;
    margin-top: 10rpx;
}

.map_nav_btn_add_icon {
    width: 60rpx;
    height: 60rpx;
    float: left;
    margin-left: 55rpx;
    margin-top: 17rpx;
}

.map_nav_btn_add_title {
    font-size: 26rpx;
    font-weight: 500;
    color: #252525;
    float: left;
    margin-top: 30rpx;
    margin-left: 50rpx;
}