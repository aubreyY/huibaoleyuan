/* 地图 */
.map_content {
    position: relative;
}

.map_nav_box {
    width: 702rpx;
    height: 272rpx;
    background: #fff;
    border-radius: 12rpx;
    position: absolute;
    bottom: 100rpx;
    left: 26rpx;
    box-shadow: 0rpx 0rpx 14rpx rgba(34, 25, 25, 0.3);
    -webkit-box-shadow: 0rpx 0rpx 14rpx rgba(34, 25, 25, 0.3);
}

/* 网点导航 */
.map_nav_tit {
    font-size: 30rpx;
    font-weight: 500;
    color: #252525;
    margin-left: 25rpx;
    margin-top: 25rpx;
}

.map_nav_time_icon,
.map_nav_near_icon {
    width: 30rpx;
    height: 30rpx;
    margin-left: 20rpx;
    margin-top: 25rpx;
    float: left;
}

.map_nav_time_num,
.map_nav_near_tit {
    width: 300rpx;
    font-size: 24rpx;
    color: #999999;
    float: left;
    margin-top: 25rpx;
    margin-left: 13rpx;
}


.map_nav_time_m {
    font-size: 24rpx;
    color: #666666;
    float: left;
    margin-top: 25rpx;
}

.icon_x {
    width: 20rpx;
    height: 20rpx;
    float: left;
    margin-top: 30rpx;
    margin-left: 340rpx;
}

.icon_d {
    width: 35rpx;
    height: 35rpx;
    float: right;
    margin-top: 20rpx;
    margin-right: 25rpx;
}

.map_nav_btn {
    width: 100%;
    height: 90rpx;
    border-top: 1rpx solid #f3f3f3;
    margin-top: 29rpx;
}

.map_nav_btn_phone {
    width: 350rpx;
    height: 90rpx;
    float: left;
}

.map_phone_icon {
    width: 60rpx;
    height: 60rpx;
    float: left;
    margin-left: 55rpx;
    margin-top: 15rpx;
}
.map_phone_title{
    font-size:26rpx;
    font-weight: 500;
    color: #252525;
    float: left;
    margin-top: 29rpx;
    margin-left: 35rpx;
}
.map_nav_border{
    width: 2rpx;
    height: 90rpx;
    float: left;
    background: #f3f3f3;
}
.map_nav_btn_add{
    width: 345rpx;
    height: 90rpx;
    float: left;
}
.map_nav_btn_add_icon{
    width: 60rpx;
    height: 60rpx;
    float: left;
    margin-left: 55rpx;
    margin-top: 15rpx;
}
.map_nav_btn_add_title{
    font-size:26rpx;
    font-weight: 500;
    color: #252525;
    float: left;
    margin-top: 29rpx;
    margin-left: 50rpx;
}