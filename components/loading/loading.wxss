
.loading3 {
    position: relative;
    width: 50rpx;
    height: 50rpx;
    margin:30rpx auto;
}
.demo3 {
    width: 4rpx;
    height: 4rpx;
    border-radius: 2rpx;
    background: #68b2ce;
    position: absolute;
    animation: demo3 linear 0.8s infinite;
    -webkit-animation: demo3 linear 0.8s infinite;
}
.demo3:nth-child(1){
    left: 24rpx;
    top: 2rpx;
    animation-delay:0s;
}
.demo3:nth-child(2){
    left: 40rpx;
    top: 8rpx;
    animation-delay:0.1s;
}
.demo3:nth-child(3){
    left: 47rpx;
    top: 24rpx;
    animation-delay:0.1s;
}
.demo3:nth-child(4){
    left: 40rpx;
    top: 40rpx;
    animation-delay:0.2s;
}
.demo3:nth-child(5){
    left: 24rpx;
    top: 47rpx;
    animation-delay:0.4s;
}
.demo3:nth-child(6){
    left: 8rpx;
    top: 40rpx;
    animation-delay:0.5s;
}
.demo3:nth-child(7){
    left: 2rpx;
    top: 24rpx;
    animation-delay:0.6s;
}
.demo3:nth-child(8){
    left: 8rpx;
    top: 8rpx;
    animation-delay:0.7s;
}

@keyframes demo3 
{
    0%,40%,100% {transform: scale(1);}
    20% {transform: scale(3);}
}
